{
  "name": "@bbc/webpack-s3-uploader",
  "version": "1.1.0",
  "description": "Webpack plugin to push assets to s3",
  "main": "dist/webpack-s3-uploader.js",
  "module": "src/webpack-s3-uploader.js",
  "scripts": {
    "pretty": "prettier --write '{src,test}/**/*.js' '*.{js,json,yml}'",
    "prelint": "yarn pretty",
    "lint": "yarn lint:only",
    "lint:only": "eslint src test",
    "test": "BABEL_ENV=unittest mocha --require @babel/register 'test/**/*.test.js'",
    "test:coverage": "nyc --reporter=html yarn test",
    "build": "webpack --mode production --config webpack.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bbc/webpack-s3-uploader.git"
  },
  "keywords": [
    "webpack",
    "s3",
    "uploader"
  ],
  "author": {
    "name": "BBC Sounds Development team",
    "email": "sounds-development@lists.forge.bbc.co.uk"
  },
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/bbc/webpack-s3-uploader/issues"
  },
  "homepage": "https://github.com/bbc/webpack-s3-uploader#readme",
  "dependencies": {
    "aws-sdk": "^2.439.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@babel/register": "^7.4.0",
    "babel-loader": "^8.0.5",
    "chai": "^4.2.0",
    "clean-webpack-plugin": "^2.0.1",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-mocha": "^5.3.0",
    "eslint-plugin-prettier": "^3.0.1",
    "husky": "^1.3.1",
    "mocha": "^6.1.3",
    "nyc": "^14.0.0",
    "prettier": "^1.17.0",
    "pretty-quick": "^1.10.0",
    "sinon": "^7.3.2",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.0",
    "webpack-node-externals": "^1.7.2"
  },
  "nyc": {
    "exclude": [
      "test"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
